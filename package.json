{
  "name": "v-putman",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "prepare": "husky install"
  },
  "dependencies": {
    "pinia": "^2.1.7",
    "vue": "^3.3.13",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@pinia/testing": "^0.1.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/jest": "^29.5.10",
    "@types/jsdom": "^21.1.3",
    "@types/mocha": "^10.0.6",
    "@types/node": "^18.18.5",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vitest/coverage-istanbul": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.20.1",
    "husky": "^8.0.0",
    "jsdom": "^22.1.0",
    "lint-staged": "^15.2.0",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "tailwindcss": "^3.3.5",
    "typescript": "~5.2.0",
    "vite": "^5.0.11",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.19"
  },
  "lint-staged": {
    "*.js": "prettier --write"
  }
}
